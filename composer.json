{
    "name": "shornuk/craft-upload",
    "description": "Front-end asset upload for user photos, fields, volumes and folders.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "asset uploader",
        "front end",
    ],
    "support": {
        "docs": "https://github.com/shornuk/craft-upload/blob/master/README.md",
        "issues": "https://github.com/shornuk/craft-upload/issues"
    },
    "license": "proprietary",
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "shornuk\\upload\\": "src/"
        }
    },
    "extra": {
        "name": "Upload",
        "handle": "upload",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/shornuk/craft-upload/master/CHANGELOG.md",
        "class": "shornuk\\upload\\Upload",
        "components": {
            "upload": "shornuk\\upload\\services\\UploadService"
        }
    }
}
